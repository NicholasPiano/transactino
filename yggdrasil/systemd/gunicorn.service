[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/thor/wotan
ExecStart=/home/ubuntu/thor/wotan/bin/gunicorn --access-logfile - --workers 1 --bind unix:/home/ubuntu/thor/wotan/wotan.sock woot.wsgi:application

[Install]
WantedBy=multi-user.target
