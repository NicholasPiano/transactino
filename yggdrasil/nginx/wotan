server {
    listen 80;
    server_name 35.178.85.150;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static {
        alias /home/ubuntu/thor/wotan/woot/static;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/ubuntu/thor/wotan/wotan.sock;
    }
}
